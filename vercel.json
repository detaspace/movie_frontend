{
  "version": 2,
  "public": true,
  "routes": [
    // 1. **सबसे विशिष्ट: डीप लिंक रूट**
    { "src": "/s/(.*)", "dest": "/index.html" },
    
    // 2. **दूसरा विशिष्ट रूट**
    { "src": "/:username/:id", "dest": "/index.html" },
    
    // 3. **.well-known राउटिंग की आवश्यकता नहीं है** - Vercel को फ़ाइलें अपने आप सर्व करनी चाहिए।
    
    // 4. **सबसे सामान्य/फॉलबैक**
    { "src": "/.*", "dest": "/index.html" }
  ]
}

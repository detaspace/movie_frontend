{
  "version": 2,
  "public": true,
  "routes": [
    {
      "src": "/\\.well-known/assetlinks\\.json",
      "dest": "/.well-known/assetlinks.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/terms",
      "dest": "/index.html"
    },
    {
      "src": "/privacy",
      "dest": "/index.html"
    },
    {
      "src": "/deleteData",
      "dest": "/index.html"
    },
    {
      "src": "/deleteAccount",
      "dest": "/index.html"
    },
    {
      "src": "/:username/:id",
      "dest": "/index.html"
    },
    {
      "src": "/app-release\\.apk",
      "dest": "/app-release.apk"
    },
    {
      "src": "/s/(.*)",
      "dest": "/index.html"
    }
  ]
}

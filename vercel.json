{
  "version": 2,
  "public": true,
  "routes": [
    { "src": "/s/(.*)", "dest": "/index.html" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/.well-known/assetlinks.json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" }
      ]
    }
  ]
}
